# ==============================================================================
# Example Variables
# ==============================================================================
# Copy this file to terraform.tfvars and fill in your values:
#   cp terraform.tfvars.example terraform.tfvars
#
# WARNING: Never commit terraform.tfvars to version control!
# ==============================================================================

# AWS region for deployment
aws_region = "us-east-1"

# Netskope API configuration
# Recommended: Set via environment variables instead
#   export TF_VAR_netskope_server_url="https://tenant.goskope.com/api/v2"
#   export TF_VAR_netskope_api_key="your-api-key"
netskope_server_url = "https://your-tenant.goskope.com/api/v2"
# netskope_api_key  = ""  # DO NOT store in this file

# Publisher configuration
publisher_name     = "npa-publisher-example"
publisher_key_name = "your-key-pair-name"
